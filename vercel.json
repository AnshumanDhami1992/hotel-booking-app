{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|json|woff|woff2|ttf|svg))",
        "dest": "/index.js"
      },
      {
        "src": "/.*",
        "dest": "/index.js"
      }
    ]
  }
  